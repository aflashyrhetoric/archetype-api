{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "kits": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::kit.kit",
      "inversedBy": "products"
    },
    "affiliate_link": {
      "type": "string"
    },
    "media_product_reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::media-product-review.media-product-review"
    },
    "pros": {
      "type": "richtext"
    },
    "cons": {
      "type": "richtext"
    },
    "is_alone_in_product_category": {
      "type": "boolean",
      "default": false
    },
    "long_description": {
      "type": "richtext"
    },
    "price": {
      "type": "decimal"
    },
    "sale_price": {
      "type": "decimal"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "the desk",
        "i/o",
        "tech",
        "monitors",
        "audio",
        "decor"
      ]
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    }
  }
}
